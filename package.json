{
  "name": "esrieact",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "turbo dev tsc:watch lint:watch",
    "lint": "eslint . --ext ts,tsx",
    "lint:watch": "onchange -i \"**/*.{tsx,ts}\" -e \"{playwright/**/*,.next/**/*}\" -- npm run lint"
  },
