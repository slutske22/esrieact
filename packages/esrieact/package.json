{
  "name": "esrieact",
  "private": false,
  "version": "0.3.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && cp ../../README.md .",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch --preserveWatchOutput"
  },
  "exports": {
    ".": "./dist/index.js",
    "./core/*": "./dist/core/*",
    "./layers/*": "./dist/layers/*",
    "./map/*": "./dist/map/*",
    "./symbols/*": "./dist/symbols/*",
    "./utils/*": "./dist/utils/*",
    "./widgets/*": "./dist/widgets/*"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "peerDependencies": {
    "@arcgis/core": "^4.30.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.2.2"
  }
}
